{
  "version": 2,
  "builds": [
    {
      "src": "frontend-react/next.config.mjs",
      "use": "@vercel/next"
    },
    {
      "src": "backend-express/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend-express/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend-react"
    }
  ]
}