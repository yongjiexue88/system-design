{
  "name": "agenticchat-monorepo",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky",
    "format:run": "node scripts/auto-format.mjs",
    "start": "npm-run-all --parallel start:backend start:frontend",
    "start:backend": "npm run dev --prefix backend-express",
    "start:frontend": "npm run start --prefix frontend-react",
    "build": "npm-run-all build:backend build:frontend",
    "build:backend": "npm run build --prefix backend-express",
    "build:frontend": "npm run build --prefix frontend-react"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5"
  }
}
